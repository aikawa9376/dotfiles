{
  "suggest.noselect": true,
  "suggest.enablePreselect": true,
  "snippets.ultisnips.enable": false,
  "snippets.snipmate.enable": true,
  "snippets.extends": {
    "php": ["html"],
    "scss": ["css"],
    "typescss": ["javascript"]
  },
  "css.colorDecorators.enable": false,
  "scss.colorDecorators.enable": false,
  "diagnostic.infoSign": "∴",
  "diagnostic.hintSign": "∴",
  "diagnostic.warningSign": "∴",
  "diagnostic.errorSign": "∴",
  "diagnostic.signOffset": 98,
  "diagnostic.refreshOnInsertMode": true,
  "codeLens.enable": true,
  "coc.preferences.colorSupport": false,
  "coc.source.tmuxcomplete.priority": 40,
  "snippets.priority": 90,
  "python.pythonPath": "python3",
  "vetur.validation.template": false,
  "list.insertMappings": {
    "<C-l>": "do:refresh",
    "<C-f>": "feedkeys:\\<C-f>",
    "<C-b>": "feedkeys:\\<C-b>",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<A-n>": "prompt:previous",
    "<A-p>": "prompt:next",
    "<C-e>": "action:e",
    "<C-t>": "do:toggleMode",
    "<C-c>": "do:exit"
  },
  "list.normalMappings": {
    "d": "action:delete",
    "<C-t>": "do:toggleMode",
    "<C-c>": "do:exit"
  },
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": [
        "vim",
        "eruby",
        "markdown"
      ]
    },
    "golang": {
      "command": "gopls",
      "args": [],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    }
  }
}
